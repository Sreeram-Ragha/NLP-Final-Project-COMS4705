      "cell_type": "code",
      "source": [],
      "metadata": {
        "id": "omD6EvL-stLM"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ==================== REVISED DATASET (IMAGES ONLY) ====================\n",
        "\n",
        "class ScienceQADataset(Dataset):\n",
        "    \"\"\"Custom dataset for ScienceQA - IMAGES ONLY\"\"\"\n",
        "\n",
        "    def __init__(self, hf_dataset, processor, model_type):\n",
        "        self.processor = processor\n",
        "        self.model_type = model_type\n",
        "\n",
        "        # Filter to only examples WITH images\n",
        "        print(\"Filtering dataset to only image-based questions...\")\n",
        "        self.dataset = [ex for ex in hf_dataset if ex['image'] is not None]\n",
        "        print(f\"Filtered dataset size: {len(self.dataset)} samples (with images)\")\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.dataset)\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        example = self.dataset[idx]\n",
        "\n",
        "        # Extract fields\n",
        "        question = example['question']\n",
        "        choices = example['choices']\n",
        "        hint = example['hint'] if example['hint'] else \"\"\n",
        "        answer_idx = example['answer']\n",
        "        image = example['image']\n",
        "\n",
        "        # Create prompt\n",
        "        prompt = create_prompt(question, choices, hint)\n",
        "        answer_letter = chr(65 + answer_idx)\n",
        "\n",
        "        # Format for LLaVA\n",
        "        if self.model_type == \"llava\":\n",
        "            text_input = f\"<image>\\n{prompt}\"\n",
        "        else:\n",
        "            text_input = prompt\n",
        "\n",
        "        text_output = f\" {answer_letter}\"  # Space before answer\n",
        "\n",
        "        # Process image\n",
        "        if not isinstance(image, Image.Image):\n",
        "            image = Image.fromarray(np.array(image))\n",
        "        if image.mode != 'RGB':\n",
        "            image = image.convert('RGB')\n",
        "\n",
        "        return {\n",
        "            'image': image,\n",
        "            'text': text_input + text_output,  # Full sequence\n",
        "        }\n",
        "\n",
        "# ==================== SIMPLE COLLATOR ====================\n",
        "\n",
        "@dataclass\n",
        "class MultimodalCollator:\n",
        "    \"\"\"Simple collator for image-only training\"\"\"\n",
        "\n",
        "    processor: AutoProcessor\n",
        "\n",
        "    def __call__(self, batch: List[Dict]) -> Dict[str, torch.Tensor]:\n",
        "        images = [item['image'] for item in batch]\n",
        "        texts = [item['text'] for item in batch]\n",
        "\n",
        "        # Process\n",
        "        inputs = self.processor(\n",
        "            images=images,\n",
        "            text=texts,\n",
        "            return_tensors=\"pt\",\n",
        "            padding=True,\n",
        "        )\n",
        "\n",
        "        # Labels = input_ids (standard LM training)\n",
        "        labels = inputs[\"input_ids\"].clone()\n",
        "        labels[labels == self.processor.tokenizer.pad_token_id] = -100\n",
        "\n",
        "        inputs[\"labels\"] = labels\n",
        "\n",
        "        return inputs\n",
        "\n",
        "# ==================== RECREATE EVERYTHING ====================\n",
        "\n",
        "print(\"Creating new datasets...\")\n",
        "train_dataset_formatted = ScienceQADataset(train_dataset_subset, processor, MODEL_TYPE)\n",
        "val_dataset_formatted = ScienceQADataset(val_dataset_subset, processor, MODEL_TYPE)\n",
        "\n",
        "print(f\"Training samples (with images): {len(train_dataset_formatted)}\")\n",
        "print(f\"Validation samples (with images): {len(val_dataset_formatted)}\")\n",
        "\n",
        "# Recreate collator\n",
        "data_collator = MultimodalCollator(processor=processor)\n",
        "\n",
        "# ==================== FIXED TRAINING ARGUMENTS ====================\n",
        "\n",
        "training_args = TrainingArguments(\n",
        "    output_dir=\"./scienceqa_finetuned\",\n",
        "    num_train_epochs=3,\n",
        "    per_device_train_batch_size=1,\n",
        "    per_device_eval_batch_size=1,\n",
        "    gradient_accumulation_steps=8,\n",
        "    learning_rate=2e-5,\n",
        "    weight_decay=0.01,\n",
        "    warmup_steps=100,\n",
        "    logging_steps=50,\n",
        "    eval_strategy=\"steps\",\n",
        "    eval_steps=200,\n",
        "    save_strategy=\"steps\",\n",
        "    save_steps=200,\n",
        "    save_total_limit=2,\n",
        "    load_best_model_at_end=True,\n",
        "    metric_for_best_model=\"eval_loss\",\n",
        "    greater_is_better=False,\n",
        "    fp16=False,  # CHANGED: Disable FP16 to avoid gradient issues\n",
        "    bf16=torch.cuda.is_available() and torch.cuda.is_bf16_supported(),  # Use BF16 if available\n",
        "    dataloader_num_workers=0,\n",
        "    remove_unused_columns=False,\n",
        "    label_names=[\"labels\"],\n",
        "    report_to=\"none\",\n",
        "    max_grad_norm=1.0,  # Add gradient clipping\n",
        ")\n",
        "\n",
        "# Recreate trainer\n",
        "trainer = Trainer(\n",
        "    model=model,\n",
        "    args=training_args,\n",
        "    train_dataset=train_dataset_formatted,\n",
        "    eval_dataset=val_dataset_formatted,\n",
        "    data_collator=data_collator,\n",
        ")\n",
        "\n",
        "# NOW train\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"Starting fine-tuning (IMAGE-ONLY samples)...\")\n",
        "print(\"=\"*50 + \"\\n\")\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "rJ8M8wkpsr97",
        "outputId": "9c48f4e8-d2fe-49ca-8623-d864da1ba0a7"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Creating new datasets...\n",
            "Filtering dataset to only image-based questions...\n",
            "Filtered dataset size: 2430 samples (with images)\n",
            "Filtering dataset to only image-based questions...\n",
            "Filtered dataset size: 259 samples (with images)\n",
            "Training samples (with images): 2430\n",
            "Validation samples (with images): 259\n",
            "\n",
            "==================================================\n",
            "Starting fine-tuning (IMAGE-ONLY samples)...\n",
            "==================================================\n",
            "\n"
          ]
        }
      ]
