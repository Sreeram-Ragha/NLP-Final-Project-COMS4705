        "dataset = load_dataset(\"derek-thomas/ScienceQA\")\n",
        "test_dataset = dataset['test']\n",
        "\n"
      ],
      "metadata": {
        "id": "PiZf-fZpvSH5"
      },
      "execution_count": null,
      "outputs": []
